      RowBox[{
       RowBox[{"\[Beta]", "[", 
        RowBox[{"L", ",", "k"}], "]"}], "*", 
       RowBox[{"SphericalBesselJ", "[", 
        RowBox[{"L", ",", 
         RowBox[{"k", "*", "5", "*", "R"}]}], "]"}]}]}], ")"}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"k", "*", "5", "*", "R", "*", 
       RowBox[{
        RowBox[{
         RowBox[{"Derivative", "[", 
          RowBox[{"0", ",", "1"}], "]"}], "[", "SphericalBesselY", "]"}], "[", 
        RowBox[{"L", ",", 
         RowBox[{"k", "*", "5", "*", "R"}]}], "]"}]}], "-", 
      RowBox[{
       RowBox[{"\[Beta]", "[", 
        RowBox[{"L", ",", "k"}], "]"}], "*", 
       RowBox[{"SphericalBesselY", "[", 
