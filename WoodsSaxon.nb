Cell["\<\
These are the same equations as above, but now we have chosen u'(0)=8 (an \
arbitrary choice).\
\>", "Text",
 CellChangeTimes->{{3.5783309216233883`*^9, 3.5783309601830482`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"s", "[", 
   RowBox[{"L_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"NDSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"u", "''"}], "[", "r", "]"}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"k", "^", "2"}], "-", 
           RowBox[{"2", "*", 
            RowBox[{"m", "/", 
             RowBox[{
              RowBox[{"(", "\[HBar]c", ")"}], "^", "2"}]}], "*", 
            RowBox[{"V", "[", "r", "]"}]}], "-", 
           RowBox[{"L", "*", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"L", "+", "1"}], ")"}], "/", 
             RowBox[{"r", "^", "2"}]}]}]}], ")"}], "*", 
         RowBox[{"u", "[", "r", "]"}]}]}], "\[Equal]", "0"}], ",", 
      RowBox[{
       RowBox[{"u", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1000", "*", "k"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}], "]"}], "\[Equal]", "0"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"u", "'"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1000", "*", "k"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}], "]"}], "\[Equal]", "8"}]}], "}"}], ",",
     "u", ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1000", "*", "k"}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}]}], ",", 
      RowBox[{"5", "*", "R"}]}], "}"}], ",", 
    RowBox[{"MaxSteps", "\[Rule]", "20000"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.578271962975191*^9, 3.578271966518187*^9}, {
   3.578272328177346*^9, 3.578272329993833*^9}, {3.578272632366748*^9, 
