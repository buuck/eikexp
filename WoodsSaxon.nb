Cell[CellGroupData[{

Cell["Solve the Schr\[ODoubleDot]dinger Equation", "Section",
 CellChangeTimes->{{3.578330624825139*^9, 3.578330682745408*^9}}],

Cell["\<\
First I solve the Schr\[ODoubleDot]dinger equation for the radial \
wavefunction with an arbitrary constant for the initial condition for u'(0). \
My goal is to show that the resulting phase shift we get from this in \
independent of the choice of u'(0).\
\>", "Text",
 CellChangeTimes->{{3.578330636985202*^9, 3.5783306505530443`*^9}, {
  3.57833069072993*^9, 3.578330760649095*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"s", "[", 
   RowBox[{"L_", ",", "k_", ",", "b_"}], "]"}], ":=", 
  RowBox[{"NDSolve", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"u", "''"}], "[", "r", "]"}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"k", "^", "2"}], "-", 
           RowBox[{"2", "*", 
            RowBox[{"m", "/", 
             RowBox[{
              RowBox[{"(", "\[HBar]c", ")"}], "^", "2"}]}], "*", 
            RowBox[{"V", "[", "r", "]"}]}], "-", 
           RowBox[{"L", "*", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"L", "+", "1"}], ")"}], "/", 
             RowBox[{"r", "^", "2"}]}]}]}], ")"}], "*", 
         RowBox[{"u", "[", "r", "]"}]}]}], "\[Equal]", "0"}], ",", 
      RowBox[{
       RowBox[{"u", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1000", "*", "k"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}], "]"}], "\[Equal]", "0"}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"u", "'"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1000", "*", "k"}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}], "]"}], "\[Equal]", "b"}]}], "}"}], ",",
     "u", ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1000", "*", "k"}], ")"}], "^", 
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}]}], ",", 
      RowBox[{"5", "*", "R"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.578271887400613*^9, 3.578271891568645*^9}, {
   3.578273520412768*^9, 3.5782735236639137`*^9}, {3.578862491597909*^9, 
   3.57886249287719*^9}, 3.578862984813072*^9, {3.580049958748026*^9, 
   3.580049964426869*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"u", "[", 
   RowBox[{"L_", ",", "k_", ",", "r_", ",", "b_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"u", "[", "r", "]"}], "/.", 
   RowBox[{
    RowBox[{"s", "[", 
     RowBox[{"L", ",", "k", ",", "b"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.578271853041113*^9, 3.578271853984568*^9}, {
  3.5782718955053596`*^9, 3.578271899542877*^9}, {3.578862526966673*^9, 
  3.578862527486005*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Beta]", "[", 
   RowBox[{"L_", ",", "k_", ",", "b_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"5", "*", 
    RowBox[{"R", "/", 
     RowBox[{"Evaluate", "[", 
      RowBox[{"u", "[", 
       RowBox[{"L", ",", "k", ",", 
        RowBox[{"5", "*", "R"}], ",", "b"}], "]"}], "]"}]}], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"u", "[", 
         RowBox[{"L", ",", "k", ",", "r", ",", "b"}], "]"}], ",", "r"}], 
       "]"}], "/.", 
